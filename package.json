{
  "name": "printdesk",
  "version": "1.51.0",
  "productName": "Printdesk",
  "description": "Hardware bridge for ServicePOS",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env ELECTRON_IS_DEV=1 electron .",
    "build": "cross-env ELECTRON_IS_DEV=0 electron-builder --win --macos",
    "pack": "cross-env ELECTRON_IS_DEV=1 electron-builder --win --macos --dir",
    "publish": "cross-env ELECTRON_IS_DEV=0 electron-builder --win --macos -p always"
  },
  "repository": "https://github.com/servicepos/printdesk",
  "build": {
    "appId": "com.servicepos.printdesk",
    "asarUnpack": [
      "assets/PDFtoPrinter.exe",
      "assets/BamdeskMint.exe",
      "assets/BamdeskMintTCP.exe",
      "assets/PDF-XChange Viewer Settings.dat"
    ],
    "mac": {
      "icon": "assets/servicepos.png"
    },
    "protocols": {
      "name": "printdesk-protocol",
      "schemes": [
        "printdesk"
      ]
    },
    "win": {
      "icon": "assets/servicepos.ico",
      "target": [
        "squirrel"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "servicepos",
        "repo": "printdesk"
      }
    ]
  },
  "postinstall": "electron-builder install-app-deps",
  "author": "ServicePOS",
  "license": "CC0-1.0",
  "devDependencies": {
    "cross-env": "^5.2.1",
    "electron": "^7.2.4",
    "electron-builder": "^20.44.4",
    "electron-builder-squirrel-windows": "^20.44.0",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-installer-zip": "^0.1.2",
    "electron-reloader": "^0.3.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "cmd-promise": "^1.2.0",
    "command-exists": "^1.2.9",
    "cors": "^2.8.5",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^3.0.9",
    "electron-prompt": "^1.5.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^3.3.0",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "node-ip": "^0.1.2",
    "node-machine-id": "^1.1.10",
    "open-file-explorer": "^1.0.2",
    "ps-node": "^0.1.6",
    "request": "^2.88.0",
    "tmp": "^0.1.0",
    "update-electron-app": "^1.5.0"
  }
}
